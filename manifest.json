{
  "manifest_version": 2,
  "name": "Keyboard Navigation",
  "version": "1.0",
  "description": "Add keyboard navigation shortcuts to all websites.",
  "icons": {
    "48": "icons/icon-48.svg",
    "96": "icons/icon-96.svg"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["build/index.js"]
    }
  ]
}
